msgid "``Backtrace_HEADER``"
msgstr ""

#: ../../CMake/Modules/FindBacktrace.cmake:14
msgid ""
"The header file needed for ``backtrace(3)``. Cached. Could be forcibly "
"set by user."
msgstr ""

#: ../../CMake/Modules/FindBacktrace.cmake:16
msgid "``Backtrace_INCLUDE_DIRS``"
msgstr ""

#: ../../CMake/Modules/FindBacktrace.cmake:17
msgid "The include directories needed to use ``backtrace(3)`` header."
msgstr ""

#: ../../CMake/Modules/FindBacktrace.cmake:18
msgid "``Backtrace_LIBRARIES``"
msgstr ""

#: ../../CMake/Modules/FindBacktrace.cmake:19
msgid "The libraries (linker flags) needed to use ``backtrace(3)``, if any."
msgstr ""

#: ../../CMake/Modules/FindBacktrace.cmake:21
msgid "``Backtrace_FOUND``"
msgstr ""

#: ../../CMake/Modules/FindBacktrace.cmake:21
msgid "Is set if and only if ``backtrace(3)`` support detected."
msgstr ""

#: ../../CMake/Modules/FindBacktrace.cmake:23
msgid "The following cache variables are also available to set or use:"
msgstr ""

#: ../../CMake/Modules/FindBacktrace.cmake:25
msgid "``Backtrace_LIBRARY``"
msgstr ""

#: ../../CMake/Modules/FindBacktrace.cmake:26
msgid "The external library providing backtrace, if any."
msgstr ""

#: ../../CMake/Modules/FindBacktrace.cmake:28
msgid "``Backtrace_INCLUDE_DIR``"
msgstr ""

#: ../../CMake/Modules/FindBacktrace.cmake:28
msgid "The directory holding the ``backtrace(3)`` header."
msgstr ""

