msgid "where ``<dir>`` is one of:"
msgstr ""

#: ../../CMake/Modules/GNUInstallDirs.cmake:39
msgid "``BINDIR``"
msgstr ""

#: ../../CMake/Modules/GNUInstallDirs.cmake:40
msgid "user executables (``bin``)"
msgstr ""

#: ../../CMake/Modules/GNUInstallDirs.cmake:41
msgid "``SBINDIR``"
